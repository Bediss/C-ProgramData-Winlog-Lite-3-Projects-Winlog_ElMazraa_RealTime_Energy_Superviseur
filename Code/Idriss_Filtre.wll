//MessageBox(NumLigne+"=NumLigne",Page+"Page");


   Ligne = FileReadLn(FileHandle);
  // MessageBox("Ligne= "+Ligne,row+"row");

    for i = 1 to NumLigne do
    Ligne = FileReadLn(FileHandle);
    end
    FileClose(FileHandle);
    //MessageBox(Ligne,"Ligne");
