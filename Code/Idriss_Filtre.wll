function void Display_Filtre(Int Page,String File,int Pos,String Gate,String Sep)

    int FileHandle; int j=0;
    string TextRow; string txt;
    int ReadRowPos=0;
    int Row_Start;  int Row_End;     bool RowHeader = false;
    String EVM;
    String Part;

    Int StartDisplay;
    int i;




           For i = 1 to 15 do
              EVM = Gate+i;
              SetStrGateValue("RowList",i,"");
           End

    MessageBox(File,"File");

    FileHandle=FileOpen(File,"rt");

    if (FileHandle==0) then
      MessageBox("Fichier "+File+" n'existe Pas","Erreur");  Return;
    end

     Row_Start = (Page - 1)*15+1;
     Row_End =  Page*15;

    StartDisplay = Row_Start -1;

     FileReadLn(FileHandle);

//MessageBox(HeaderLigneFiltre,"HeaderLigneFiltre");
    For i = 1 to StartDisplay do
        FileReadLn(FileHandle);
    End

    j = 0;
    For i = Row_Start to Row_End do

        TextRow = FileReadLn(FileHandle);
        Part=Fetch_Email(TextRow,Pos,Sep);
              j = j+1;
                EVM = Gate+j;

                SetStrGateValue("RowList",j,Part);

     end //for display

     FileClose(FileHandle);

     if ( j== 0 ) then
        MessageBox("Il n'y a plus de pages disponibles","Information");
        CleanEmptyRows(j+1,Gate);
      else
        CleanEmptyRows(j+1,Gate);
     end

end



Function void SearchConfigFildFiltre(int PosHeader , String Value,String TypeSearch)
int N=Occurence_operateur(HeaderSearch,";");
int i;
String Part;
bool Change=False;
//MessageBox(Filter_Counter+"=Filter_Counter",HeaderSearch+"=Header");
//MessageBox(Value+"=Value",HeaderSearch+"=Header");

        if TypeSearch=="text" then
        Value="%"+Value+"%";
        end

         if Value=="" then return; end
    IF OFF_FILTRE==True then
       if FetchBU(HeaderLigneFiltre,PosHeader,";")==FetchBU(HeaderLigneFiltre,3,";") then /* Position de "Report_TableauCode" Hedeat ==3 */
            //SetStrGateValue("BU_Template",0,"");
            SetStrGateValue("BU_Energy",0,"");
            SetStrGateValue("EV_Measure",0,"");
            SetStrGateValue("AlarmeEventModifer",29,"");
            SetStrGateValue("AlarmeEventModifer",28,"");

                //MessageBox(Header +" = Report_TableauCode",Filter_Counter);

        else if FetchBU(HeaderLigneFiltre,PosHeader,";")==FetchBU(HeaderLigneFiltre,8,";") then /* Position de "Report_ViewName" Hedeat ==8*/
            SetStrGateValue("BU_Template",0,"");
            SetStrGateValue("BU_Energy",0,"");
            //SetStrGateValue("EV_Measure",0,"");
            SetStrGateValue("AlarmeEventModifer",29,"");
            SetStrGateValue("AlarmeEventModifer",28,"");

            //MessageBox(Header +" = Report_ViewName",Filter_Counter);
            end
        else if  FetchBU(HeaderLigneFiltre,PosHeader,";")==FetchBU(HeaderLigneFiltre,2,";") then /* Position de "Report Name" Hedeat ==2 */
            SetStrGateValue("BU_Template",0,"");
            SetStrGateValue("BU_Energy",0,"");
            SetStrGateValue("EV_Measure",0,"");
            //SetStrGateValue("AlarmeEventModifer",29,"");
            SetStrGateValue("AlarmeEventModifer",28,"");
              //MessageBox(Header +" = Report Name",Filter_Counter);
         end
           else if  FetchBU(HeaderLigneFiltre,PosHeader,";")==FetchBU(HeaderLigneFiltre,10,";") then /* Position de "Report_PostC_Name" Hedeat ==10 */
            SetStrGateValue("BU_Template",0,"");
            SetStrGateValue("BU_Energy",0,"");
            SetStrGateValue("EV_Measure",0,"");
            SetStrGateValue("AlarmeEventModifer",29,"");
           // SetStrGateValue("AlarmeEventModifer",28,"");
            // MessageBox(Header +" = Report_PostC_Name",Filter_Counter);
         end
           else if  FetchBU(HeaderLigneFiltre,PosHeader,";")==FetchBU(HeaderLigneFiltre,6,";") then /* Position de "Report_EnergyName" Hedeat ==6 */
                SetStrGateValue("BU_Template",0,"");
           // SetStrGateValue("BU_Energy",0,"");
            SetStrGateValue("EV_Measure",0,"");
            SetStrGateValue("AlarmeEventModifer",29,"");
            SetStrGateValue("AlarmeEventModifer",28,"");
          // MessageBox(Filter_Counter,Header " = BU_Energy");
              // MessageBox(Header +" = Report_PhtttttostC_Name",Filter_Counter);
               end
         end
        end
       // MessageBox(FetchBU(HeaderLigneFiltre,PosHeader,";"),"heder");
      if Filter_Counter != 0 then
        for i = 1 to N do

            if FetchBU(HeaderSearch,i,";")==FetchBU(HeaderLigneFiltre,PosHeader,";") then
                FR_Formule(ValueSearch,Value,i);
                 Filter_Counter=Filter_Counter-1;
            end
        end
            HeaderSearch=HeaderSearch+";"+FetchBU(HeaderLigneFiltre,PosHeader,";");
            ValueSearch=ValueSearch+";"+Value;

      else
             HeaderSearch=FetchBU(HeaderLigneFiltre,PosHeader,";");
             ValueSearch=Value;
            //  MessageBox(ValueSearch,HeaderSearch);

      end
    Filter_Counter=Filter_Counter+1;

END

function String FR_Filtre(String Ligne,string Replace , Int NumFild)

string txt = Ligne;
String NewLigne ="";
String CODE = "";
int i;  int P = 0;


      for i =1  to NumFild-1 do
        P = StrPos(txt,";");

        CODE=StrSubString(txt,1,P);
       NewLigne = NewLigne + CODE ;
        txt=StrDelete(txt,1,P);
      end

        P = StrPos(txt,";");
        CODE=StrSubString(txt,1,P-1);
        txt=StrDelete(txt,1,P-1);

       P = StrPos(CODE,"_");
        Return NewLigne + Replace + txt;


end
